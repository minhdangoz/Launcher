<?xml version="1.0" encoding="utf-8"?>
<com.android.launcher3.XDockView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.android.launcher3.XDockViewContainer
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.android.launcher3.XDockViewLayout
            android:id="@+id/dock_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
        </com.android.launcher3.XDockViewLayout>

    </com.android.launcher3.XDockViewContainer>

</com.android.launcher3.XDockView>